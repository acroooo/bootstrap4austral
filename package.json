{
  "name": "bootstrap4austral",
  "version": "1.0.0",
  "description": "Proyecto para la Universidad Austral -  Bootstrap4",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin nosotros.html -d dist --htmlmin -o dist/nosotros.html && usemin sedes.html -d dist --htmlmin -o dist/sedes.html && usemin servicios.html -d dist --htmlmin -o dist/servicios.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acroooo/bootstrap4austral.git"
  },
  "author": "Hern√°n Chamorro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acroooo/bootstrap4austral/issues"
  },
  "homepage": "https://github.com/acroooo/bootstrap4austral#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "grunt": "^1.3.0",
    "htmlmin": "0.0.7",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^3.0.2",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.16.1"
  }
}
